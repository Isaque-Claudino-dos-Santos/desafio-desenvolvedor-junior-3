generator client {
  provider        = "prisma-client-js"
  output          = "../api/generated/client"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String?

  @@map("users")
}
